{
    "entities": [
        {
            "name": "subject",
            "pattern": "(sub-\\w+)",
            "directory": "{{root}}/{subject}"
        },
        {
            "name": "session",
            "pattern": "(ses-\\d)",
            "mandatory": false,
            "directory": "{{root}}/{subject}/{session}",
            "missing_value": "ses-1"
        },
        {
            "name": "run",
            "pattern": "(run-\\d+)"
        },
        {
            "name": "type",
            "pattern": ".*_(.*?)\\."
        },
        {
            "name": "task",
            "pattern": "task-(.*?)(?:_+)"
        },
        {
            "name": "acquisition",
            "pattern": "acq-(.*?)(?:_+)"
        },
        {
            "name": "bval",
            "pattern": "(.*\\.bval)$"
        },
        {
            "name": "bvec",
            "pattern": "(.*\\.bvec)$"
        },
        {
            "name": "ext",
            "pattern": "(\\.\\w+\\.\\w+|\\.\\w+)$"
        },
        {
            "name": "fmap",
            "pattern": "(phasediff|magnitude[0-9]*|phase[0-9]+|fieldmap|epi)\\.nii"
        },
        {
            "name": "modality",
            "pattern": "\/(func|anat|fmap|dwi|asl|perf)\/"
        }
    ]
}
